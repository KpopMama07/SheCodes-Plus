console.log(australia[2]);

//Challenge 4

let cities = [
	{
		name: "Seoul",
		country: "Korea",
		language: "Korean",
		temperature: 30,
		capitalCity: true
	},
	{
		name: "London",
		country: "England",
		language: "English",
		temperature: 20,
		capitalCity: true
	},
	{
		name: "Tokyo",
		country: "Japan",
		language: "Japanese",
		temperature: 10,
		capitalCity: true
	}
];

console.log(cities[0].temperature);
console.log(cities[1].temperature);
console.log(cities[2].temperature);
